# Filter the data for 3rd and 6th replications
filtered_data <- noise[noise$Recording %in% c(3, 6), ]

# Perform a t-test comparing the two groups
t_test_result <- t.test(Volume ~ Recording, data = filtered_data)

# View the result of the t-test
print(t_test_result)


# Load necessary library
library(ggplot2)

# Ensure that 'Place' is a factor
noise$Place <- factor(noise$Place, levels = c("At site", "50 meters", "100 meters"))

# Perform ANOVA comparing noise levels across the three places
anova_result <- aov(Volume ~ Place, data = noise)

# View the summary of the ANOVA result
summary(anova_result)
